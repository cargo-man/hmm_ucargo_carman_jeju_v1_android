<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="50dp"
    android:background="@drawable/line1_1"
    android:padding="10dp">
    <TextView
        android:id="@+id/adpater_jibu_name"
        android:layout_marginTop="10dp"
        style="@style/hh_info3"
        android:textSize="16dp"
        android:layout_centerVertical="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="중앙지부(수원시, 오산시, 화성시)"/>
    <TextView
        android:id="@+id/adpater_jibu_address"
        android:layout_marginTop="10dp"
        style="@style/hh_info2"
        android:textSize="14dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="수원시 장안구 만석로 210번길 66, 2층 (송죽동)"/>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            style="@style/hh_info2"
            android:textSize="14dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="T "/>
        <TextView
            android:id="@+id/adpater_jibu_tel1"
            style="@style/hh_info2"
            android:textSize="14dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="031-000-0000"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="3dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            style="@style/hh_info2"
            android:textSize="14dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="F "/>
        <TextView
            android:id="@+id/adpater_jibu_fax"
            style="@style/hh_info2"
            android:textSize="14dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="031-000-0000"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="right|center_vertical">
        <TextView
            android:id="@+id/adpater_jibu_mapgo"
            style="@style/userText06"
            android:text="@string/jibu_gomap"
            android:textSize="14dp"
            android:background="@drawable/cbutton_map"
            android:paddingLeft="35dp"
            android:paddingBottom="2dp"
            android:layout_width="100dp"
            android:layout_height="31dp"
            android:layout_marginRight="10dp"/>
        <TextView
            android:id="@+id/adpater_jibu_telgo"
            style="@style/userText06"
            android:text="@string/jibu_gocall"
            android:textSize="14dp"
            android:background="@drawable/cbutton_call"
            android:paddingLeft="35dp"
            android:paddingBottom="2dp"
            android:layout_width="100dp"
            android:layout_height="31dp" />
    </LinearLayout>
</LinearLayout>