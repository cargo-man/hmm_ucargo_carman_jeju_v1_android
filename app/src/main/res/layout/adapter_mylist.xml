<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/adapter_mylist_box"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/colorUser17">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">
        <LinearLayout
            android:id="@+id/adapter_mylist_titlebox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerVertical="true"
            android:layout_marginRight="90dp">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom">
                <TextView
                    android:id="@+id/adapter_mylist_startdate"
                    style="@style/userText07"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:text="11.22(월)"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/adapter_mylist_corpname"
                    style="@style/userText07"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:text="화주명"
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">
                <TextView
                    android:id="@+id/adapter_mylist_loadarea"
                    style="@style/userText01"
                    android:lines="1"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textSize="13dp"
                    android:text="인천 서구 가좌동"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:src="@drawable/ico_arrow"
                    android:scaleType="center"
                    android:layout_width="10dp"
                    android:layout_height="10dp" />
                <TextView
                    android:id="@+id/adapter_mylist_downarea"
                    style="@style/userText01"
                    android:lines="1"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textSize="13dp"
                    android:text="인천 서구 가좌동"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true">
            <TextView
                android:id="@+id/adapter_mylist_paynoget"
                style="@style/userText06"
                android:gravity="center"
                android:background="@drawable/rbox_type3_2"
                android:text="@string/mylist_paynoget"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginLeft="10dp"/>
            <TextView
                android:id="@+id/adapter_mylist_payget"
                style="@style/userText06"
                android:gravity="center"
                android:background="@drawable/rbox_type3_1"
                android:text="@string/mylist_payget"
                android:layout_marginLeft="10dp"
                android:layout_width="55dp"
                android:layout_height="55dp" />
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center_vertical">
        <LinearLayout
            android:id="@+id/adapter_mylist_feebox"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:layout_marginRight="150dp">
            <ImageView
                android:src="@drawable/round_type1_4"
                android:scaleType="fitXY"
                android:layout_width="2dp"
                android:layout_height="2dp"
                android:layout_marginRight="9dp"/>
            <TextView
                style="@style/userText14"
                android:textSize="13dp"
                android:layout_marginRight="5dp"
                android:text="@string/listview_fee"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/adapter_mylist_fee"
                style="@style/userText01"
                android:textSize="14dp"
                android:text="50,000"
                android:singleLine="true"
                android:maxLines="1"
                android:ellipsize="end"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"/>
            <TextView
                android:id="@+id/adapter_mylist_feestatus"
                style="@style/userText14"
                android:textColor="@color/colorListPay3"
                android:textSize="13dp"
                android:text="(지급완료)"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingRight="5dp"
            android:layout_alignParentRight="true">
            <TextView
                android:id="@+id/adapter_mylist_paytype"
                android:background="@drawable/rbox_type3_6"
                android:padding="2dp"
                android:textSize="13dp"
                android:text="@string/list_pay6"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:minWidth="40dp"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"/>
            <TextView
                android:id="@+id/adapter_mylist_pay"
                style="@style/userText08"
                android:textSize="19dp"
                android:text="360,000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </RelativeLayout>
    <ImageView
        android:background="@color/colorUser8"
        android:layout_width="match_parent"
        android:layout_height="1sp" />
</LinearLayout>